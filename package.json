        "test": "node ./scripts/test.js",
        "preparePackage": "node ./scripts/preparePackage.js"
    },
    "devDependencies": {
        "@types/mocha": "^8.2.2",
        "@types/node": "^22.7.5",
        "@types/shelljs": "^0.8.15",
        "@types/sinon": "^10.0.13",
        "@types/uuid": "^9.0.8",
        "mocha": "^10.0.0",
        "nyc": "^15.1.0",
        "shelljs": "^0.8.5",
        "sinon": "^15.0.1",
        "sync-request": "^6.1.0",
        "tfx-cli": "^0.21.3",
        "tslint": "^5.20.1",
        "typescript": "^4.2.4"
    },
    "nyc": {
        "exclude": "**/test/*"
    }
