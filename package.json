{
    "name": "matlab-azure-devops-extension",
    "version": "1.0.0",
    "scripts": {
        "bat": "npm run clean && npm run build && npm run test",
        "clean": "node ./scripts/clean.js",
        "lint": "tslint --project .",
        "compile": "node ./scripts/compile.js",
        "copyResources": "node ./scripts/copyResources.js",
